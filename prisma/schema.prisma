// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model User {
  id         String    @id @unique @map("_id")
  name       String
  password   String
  email      String    @unique
  insertedAt DateTime? @default(now())
}
